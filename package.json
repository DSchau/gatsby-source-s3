{
  "name": "gatsby-source-s3",
  "description":
    "A Gatsby source plugin to query against an S3 bucket (including images!)",
  "version": "0.0.0",
  "main": "index.js",
  "author": "Dustin Schau <dustinschau@gmail.com> (https://dustinschau.com)",
  "repository": "https://github.com/DSchau/gatsby-source-s3.git",
  "license": "MIT",
  "keywords": ["gatsby", "gatsby-plugin", "gatsby-source-plugin"],
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-plugin-transform-object-rest-spread": "~6.26.0",
    "babel-preset-env": "~1.6.1",
    "better-queue": "~3.8.7",
    "cross-env": "~5.1.4",
    "dotenv": "~5.0.1",
    "husky": "~0.14.3",
    "jest": "~22.4.3",
    "prettier": "~1.11.1",
    "pretty-quick": "~1.4.1"
  },
  "scripts": {
    "precommit": "pretty-quick --staged",
    "build": "babel src --out-dir . --ignore __tests__",
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "test": "npm run test:unit",
    "test:unit": "jest --config jest.config.js",
    "test:e2e": "jest --config e2e-tests/jest.config.js",
    "watch": "babel -w src --out-dir . --ignore __tests__"
  },
  "dependencies": {
    "aws-sdk": "^2.224.1",
    "gatsby-source-filesystem": "~1.5.32",
    "lodash.omit": "~4.5.0",
    "yup": "^0.24.1"
  }
}
